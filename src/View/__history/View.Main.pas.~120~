procedure TfrmMain.TestarAbstractFactory;
var
  LfrmAbstractFactory: TfrmAbstractFactory;
begin
  LfrmAbstractFactory := TfrmAbstractFactory.Create(Nil);
  try
    LfrmAbstractFactory.ShowModal;
  finally
    FreeAndNil(LfrmAbstractFactory);
  end;
end;

